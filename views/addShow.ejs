<div class="row mt-5">
  <div class="col-md-6 m-auto">
    <div class="card card-body">
      <h1 class="text-center mb-3">
        Enter The Time
      </h1>
      <form action="/admin/addShow" method="POST">
        <div class="form-group"></div>
          <label for="name">Time</label>
          <input
            type="name"
            id="time"
            name="time"
            class="form-control"
            placeholder="21:00"
          />
        </div>
        <div class="container">
            <label for="releasedate">Date:</label>
            <input type="date" id="date" name="date" />
          </div>
        <div class="form-group">
          <label for="name">Enter The Seats Available</label>
          <input
            type="name"
            id="seatsavail"
            name="seatsavail"
            class="form-control"
            placeholder="50"
          />
        </div>
        <button type="submit" class="btn btn-primary btn-block">
          Add
        </button>
      </form>
    </div>
  </div>
</div>
